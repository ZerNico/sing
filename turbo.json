{
  "$schema": "https://turbo.build/schema.json",

  "pipeline": {
    "build": {
      "dependsOn": ["^build", "db:generate"],
      "outputs": ["dist/**", ".output/**"]
    },
    "release": {
      "dependsOn": ["^build", "db:generate"],
      "outputs": ["dist/**", ".output/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["db:generate"],
      "cache": false
    },
    "start": {
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "test:e2e": {},
    "db:generate": {
      "cache": false
    }
  }
}
