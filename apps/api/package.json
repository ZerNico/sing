{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "src": "src",
  "exports": {
    "./error": "./src/error/index.ts",
    "./db": "./src/db/index.ts"
  },
  "typesVersions": {
    "*": {
      "error": [
        "./src/error/index.ts"
      ],
      "db": [
        "./src/db/index.ts"
      ]
    }
  },
  "scripts": {
    "dev": "dotenv -- node --watch --loader ts-node/esm src/index.ts",
    "build": "rimraf dist && rollup --config rollup.config.ts --configPlugin rollup-plugin-ts",
    "start": "dotenv -- node dist/index.js",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.0.1",
    "@hono/zod-validator": "^0.1.3",
    "@lucia-auth/adapter-postgresql": "2.0.0-beta.5",
    "database": "workspace:*",
    "dotenv-cli": "^7.2.1",
    "hono": "^3.2.7",
    "jose": "^4.14.4",
    "lucia": "^2.0.0-beta.4",
    "postgres": "^3.3.5",
    "rimraf": "^5.0.1",
    "semver": "^7.5.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/semver": "^7.5.0",
    "eslint-config-custom": "workspace:*",
    "rollup": "^3.26.2",
    "rollup-plugin-ts": "^3.2.0",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^5.1.6"
  }
}
