{
  "name": "api",
  "version": "0.0.1",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "dev": "dotenv -e .env -- ts-node-dev ./src/server.ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "dotenv -e .env -- node ./dist/server.js",
    "db:generate": "prisma generate",
    "db:push": "dotenv -e .env prisma db push",
    "db:migrate:dev": "dotenv -e .env -- prisma migrate dev",
    "db:migrate:deploy": "dotenv -e .env -- prisma migrate deploy",
    "db:reset": "dotenv -e .env -- prisma migrate reset",
    "start:prod": "pnpm run db:migrate:deploy && pnpm run start"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@fastify/helmet": "^10.1.0",
    "@fastify/sensible": "^5.2.0",
    "@prisma/client": "latest",
    "@trpc/server": "^10.9.0",
    "croner": "^5.7.0",
    "dotenv-cli": "^7.0.0",
    "fastify": "^4.12.0",
    "jsonwebtoken": "^9.0.0",
    "ofetch": "^1.0.0",
    "superjson": "^1.12.1",
    "typescript": "^4.9.4",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "eslint": "^8.32.0",
    "eslint-config-custom": "workspace:*",
    "prisma": "^4.11.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "workspace:*"
  }
}
