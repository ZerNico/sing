{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:allow-open",
    "fs:read-dirs",
    "fs:read-files",
    "fs:read-meta",
    "core:window:allow-set-fullscreen",
    "shell:allow-open"
  ]
}
